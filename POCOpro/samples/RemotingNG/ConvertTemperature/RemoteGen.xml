<AppConfig>
	<RemoteGen>
		<files>
			<include>
				${POCO_BASE}/include/Poco/RemotingNG/RemoteObject.h
				${POCO_BASE}/include/Poco/RemotingNG/Proxy.h
				${POCO_BASE}/include/Poco/RemotingNG/Skeleton.h
				include/WebServiceX/ConvertTemperature.h
			</include>
		</files>
		<output>
			<mode>client</mode>
			<namespace>WebServiceX</namespace>
			<include>include/WebServiceX</include>
			<src>src</src>
			<includeRoot>include</includeRoot>
			<flatIncludes>false</flatIncludes>
			<timestamps>false</timestamps>
		</output>
		<compiler id="gcc">
			<exec>g++</exec>
			<options>
				-I${POCO_BASE}/include
				-I./include
				-E
				-C
				-DPOCO_NO_GCC_API_ATTRIBUTE
				-o%.i
			</options>
		</compiler>
		<compiler id="clang">
			<exec>clang++</exec>
			<options>
				-I${POCO_BASE}/include
				-I./include
				-E
				-C
				-xc++
				-DPOCO_NO_GCC_API_ATTRIBUTE
				-o%.i
			</options>
		</compiler>
		<compiler id="msvc">
			<exec>cl</exec>
			<options>
				/I 
				"${POCO_BASE}\include"
				/I 
				".\include"
				/nologo
				/C
				/P
				/TP
			</options>
		</compiler>
	</RemoteGen>
</AppConfig>
