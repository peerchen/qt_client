<AppConfig>
	<RemoteGen>
		<files>
			<include>
				${POCO_BASE}/include/Poco/RemotingNG/RemoteObject.h
				${POCO_BASE}/include/Poco/RemotingNG/Proxy.h
				${POCO_BASE}/include/Poco/RemotingNG/Skeleton.h
				../PizzeriaREST/include/Pizzeria/OrderEndpoint.h
				../PizzeriaREST/include/Pizzeria/OrderCollectionEndpoint.h
				../PizzeriaREST/include/Pizzeria/PizzaCollectionEndpoint.h
				../PizzeriaREST/include/Pizzeria/ToppingCollectionEndpoint.h
				../PizzeriaREST/include/Pizzeria/Pizza.h
				../PizzeriaREST/include/Pizzeria/Topping.h
				../PizzeriaREST/include/Pizzeria/Order.h
				../PizzeriaREST/include/Pizzeria/DeliveryAddress.h
			</include>
			<exclude>
			</exclude>
		</files>
		<output>
			<include>include/Pizzeria</include>
			<includeRoot>include</includeRoot>
			<src>src</src>
			<namespace>Pizzeria</namespace>
			<copyright>Copyright (c) 2016, Applied Informatics Software Engineering GmbH.</copyright>
			<mode>server</mode>
			<timestamps>false</timestamps>
		</output>
		<compiler id="gcc">
			<exec>g++</exec>
			<options>
				-I${POCO_BASE}/include
				-I./include
				-I../PizzeriaREST/include
				-E
				-C
				-DPOCO_NO_GCC_API_ATTRIBUTE
				-o%.i
			</options>
		</compiler>
		<compiler id="clang">
			<exec>clang++</exec>
			<options>
				-I${POCO_BASE}/include
				-I./include
				-I../PizzeriaREST/include
				-E
				-C
				-DPOCO_NO_GCC_API_ATTRIBUTE
				-xc++
				-o%.i
			</options>
		</compiler>
		<compiler id="msvc">
			<exec>cl</exec>
			<options>
				/I 
				"${POCO_BASE}\include"
				/I 
				".\include"
				/I 
				"..\PizzeriaREST\include"
				/nologo
				/C
				/P
				/TP
			</options>
		</compiler>
	</RemoteGen>
</AppConfig>
